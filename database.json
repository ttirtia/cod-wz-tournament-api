{
  "sql-file": true,
  "defaultEnv": { "ENV": "NODE_ENV" },
  "dev": {
    "driver": "pg",
    "user": { "ENV": "DB_USERNAME" },
    "password": { "ENV": "DB_PASSWORD" },
    "host": { "ENV": "DB_HOST" },
    "database": { "ENV": "DB_NAME" },
    "port": { "ENV": "DB_PORT" },
    "ssl": false,
    "schema": { "ENV": "DB_SCHEMA" }
  },
  "production": {
    "driver": "pg",
    "user": { "ENV": "DB_USERNAME" },
    "password": { "ENV": "DB_PASSWORD" },
    "host": { "ENV": "DB_HOST" },
    "database": { "ENV": "DB_NAME" },
    "port": { "ENV": "DB_PORT" },
    "ssl": true,
    "schema": { "ENV": "DB_SCHEMA" }
  }
}
