{
  "sql-file": true,
  "defaultEnv": { "ENV": "NODE_ENV" },
  "dev": {
    "driver": "pg",
    "user": { "ENV": "PGUSER" },
    "password": { "ENV": "PGPASSWORD" },
    "host": { "ENV": "PGHOST" },
    "database": { "ENV": "PGDATABASE" },
    "port": { "ENV": "PGPORT" },
    "ssl": false,
    "schema": { "ENV": "PGSCHEMA" }
  },
  "production": {
    "driver": "pg",
    "user": { "ENV": "PGUSER" },
    "password": { "ENV": "PGPASSWORD" },
    "host": { "ENV": "PGHOST" },
    "database": { "ENV": "PGDATABASE" },
    "port": { "ENV": "PGPORT" },
    "ssl": true,
    "schema": { "ENV": "PGSCHEMA" }
  }
}
